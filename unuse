<nav class="navbar navbar-expand-lg navbar-dark">
    <a class="navbar-brand me-auto" href="#">
        <img class="web-logo" src="./resources/image/Group 2 EEET 2099.png" alt="brand logo (our team decide later)"
            onclick="index.html" ;>
    </a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">

        <ul class="navbar-nav ms-auto nav-icons">
            <% navElements=navElements.map(el=> {
                if (el.dropdown) {
                return `<li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        ${item.icon} ${item.title}
                    </a>
                    <ul class="dropdown-menu dropdown-menu-dark">
                        ${item.dropdownContent.map(subItem => {
                        return `<li><a class="dropdown-item" href="${subItem.url}">${subItem.title}</a></li>`
                        }).join('\n ')}
                    </ul>
                </li>`;
                }
                else {
                return `<li class="nav-item">
                    <a class="nav-link" href="${item.url}">${item.icon} ${item.title}</a>
                </li>`;
                }}).join('\n ')}
                }) %>
        </ul>
    </div>
</nav>

<% if (item.dropdown) { %>
    <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" role="button" data-bs-toggle="dropdown"
            aria-expanded="false">
            <%= item.icon %> <%= item.title %>
        </a>
        <ul class="dropdown-menu dropdown-menu-dark">
            <% item.dropdownContent.forEach(subItem => { %>
                <li><a class="dropdown-item" href="<%= subItem.url %>"><%= subItem.title %></a></li>
            <% }) %>
        </ul>
    </li>
<% } else { %>
    <li class="nav-item">
        <a class="nav-link" href="<%= item.url %>"><%= item.icon %> <%= item.title %></a>
    </li>
<% } %>