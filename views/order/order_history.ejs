<div class="container-xl">
  Order history page
</div>
<%
  if(orders){
    if(orders.length > 0) {
      orders.forEach(function(order){
%>
        <div>
            <h2>Order <%= order.id %></h2>
            <p><%= order.user_name %></p>
            <p><%= order.display_name %></p>
            <p><%= order.status %></p>
            <p><%= order.total_price %></p>
            <td></td>
        </div>
  <%
      });
    }
  }
  else {
    %>
      <div>No Data Found</div>
    <%
  }
%>